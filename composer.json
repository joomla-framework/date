{
    "name": "joomla/date",
    "type": "joomla-package",
    "description": "Joomla Date Package",
    "keywords": ["joomla", "framework", "date"],
    "homepage": "https://github.com/joomla-framework/date",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": "^7.2.5|~8.0.0|~8.1.0"
    },
    "require-dev": {
        "joomla/coding-standards": "~2.0@alpha",
        "phpunit/phpunit": "~6.3"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Date\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\Date\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
	        "dev-2.0-dev": "2.0-dev"
        }
    }
}
